<head>
  <link rel="stylesheet" href="{{=it.path}}/templates/style/normalize.css">
  <link rel="stylesheet" href="{{=it.path}}/templates/style/skeleton.css">
  <link rel="stylesheet" href="{{=it.path}}/templates/style/reports.css">
  <meta charset="utf-8" />
</head>

<body>
  <hgroup class="text-uppercase">
      <h6>{{=it.options.enterprise.name}}</h5>
      <h6>{{=it.i18nBilan.ACCOUNTING_DEPT }}</h6>      
  </hgroup>

  <h6 class="clean-title">
    {{=it.i18nBilan.TITLE}} {{=it.options.fy_txt}}
  </h6>

  <section>
    <!-- asset side -->
      <table class="clean-table" border="1">
        <thead>
          <tr>
            <th>{{=it.i18nBilan.REFERENCE}}</th>
            <th>{{=it.i18nBilan.ACTIF}}</th>
            <th colspan="3">Exercice N</th>
            <th>Ex. N-1</th>
          </tr>

          <tr>
            <th colspan="2"></th>
            <th>{{=it.i18nBilan.BRUT}}</th>
            <th>{{=it.i18nBilan.AMORT_PROV}}</th>
            <th>{{=it.i18nBilan.NET}}</th>
            <th>{{=it.i18nBilan.NET}}</th>
          </tr>
        </thead>
        <tbody>
          {{~it.assetSide : section : ind1}}
            <tr>
              <td></td>
              <td class="text-center"><b>{{=section.sectionBilanLabel}}({{=section.sectionBilanPosition}})</b></td>
              <td colspan="3"></td>
              <td></td>
            </tr>
              {{~section.grefs : gref : ind2}}
                <tr>
                  <td class="text-center"><b>{{=gref.greferenceAbbr}}</b></td>
                  <td><b>{{=gref.greferenceLabel}}</b></td>
                  <td colspan="3"></td>
                  <td></td>
                </tr>
                  {{~gref.refs : ref : ind3}}
                    <tr>
                      <td class="text-center">{{=ref.referenceAbbr}}</td>
                      <td>{{=ref.referenceLabel}}</td>
                      <td class="text-center">{{=ref.brut_view}}</td>
                      <td class="text-center">{{=ref.amort_prov_view}}</td>
                      <td class="text-center">{{=ref.net_view}}</td>
                      <td class="text-center">{{=ref.previousNet_view}}</td>
                    </tr>
                  {{~}}
              {{~}}
              <tr class="text-center">
                <td></td>
                <td><b>{{=it.i18nBilan.TOTAL}} {{=section.sectionBilanLabel}} ({{=section.sectionBilanPosition}})</b></td>
                <td><b>{{=section.totalBrut_view}}</b></td>
                <td><b>{{=section.totalAmortProv_view}}</b></td>
                <td><b>{{=section.totalNet_view}}</b></td>
                <td><b>{{=section.totalPreviousNet_view}}</b></td>
              </tr>
          {{~}}
          <tr class="text-center">
            <td></td>
            <td><b>{{=it.i18nBilan.TOTAL_GENERAL}}</b></td>
            <td><b>{{=it.assetGeneralBrut}}</b></td>
            <td><b>{{=it.assetGeneralAmortProv}}</b></td>
            <td><b>{{=it.assetGeneralNet}}</b></td>
            <td><b>{{=it.assetGeneralPreviousNet}}</b></td>
          </tr>
        </tbody>
      </table>

    <!-- passive side -->

      <table class="clean-table" border="1">
        <thead>
          <tr>
            <th>{{=it.i18nBilan.REFERENCE}}</th>
            <th>{{=it.i18nBilan.PASSIF}}</th>
            <th>Exercice N</th>
            <th>Ex. N-1</th>
          </tr>
        </thead>
        <tbody>
          {{~it.passiveSide : section : ind1}}
            <tr>
              <td></td>
              <td class="text-center"><b>{{=section.sectionBilanLabel}}({{=section.sectionBilanPosition}})</b></td>
              <td colspan="2"></td>
            </tr>
              {{~section.grefs : gref : ind2}}
                <tr>
                  <td class="text-center"><b>{{=gref.greferenceAbbr}}</b></td>
                  <td><b>{{=gref.greferenceLabel}}</b></td>
                  <td colspan="2"></td>
                </tr>
                  {{~gref.refs : ref : ind3}}
                    <tr>
                      <td class="text-center">{{=ref.referenceAbbr}}</td>
                      <td>{{=ref.referenceLabel}}</td>
                      <td class="text-center">{{=ref.net_view}}</td>
                      <td class="text-center">{{=ref.previousNet_view}}</td>
                    </tr>
                  {{~}}
              {{~}}
              <tr class="text-center">
                <td></td>
                <td><b>{{=it.i18nBilan.TOTAL}} {{=section.sectionBilanLabel}} ({{=section.sectionBilanPosition}})</b></td>
                <td><b>{{=section.totalNet_view}}</b></td>
                <td><b>{{=section.totalPreviousNet_view}}</b></td>
              </tr>
          {{~}}
          <tr class="text-center">
            <td></td>
            <td><b>{{=it.i18nBilan.TOTAL_GENERAL}}</b></td>
            <td><b>{{=it.passiveGeneralNet}}</b></td>
            <td><b>{{=it.passiveGeneralPreviousNet}}</b></td>
          </tr>
        </tbody>
      </table>
  </section>
</body>
