<head>
  <link rel="stylesheet" href="{{=it.path}}/templates/style/reports.css">
</head>
<body>
  <table>
    <thead>
      <tr>
        <th>Code</th>
        <th>Description</th>
        <th>Quanity</th>
        <th>Transaction Price</th>
        <th>Amount</th>
      </tr>
    </thead>
    <tbody>
    
    {{~it.invoice :saleLine:index}} 
      <tr>
        <td>{{=saleLine.code}}</td>
        <td>{{=saleLine.text}}</td>
        <td>{{=saleLine.quantity}}</td>
        <td>{{=saleLine.transaction_price}}</td>
        <td>{{=saleLine.transaction_price * saleLine.quantity}}</td>
      </tr>
    {{~}}
    </tbody>
  </table>
</body>
